@model IEnumerable<dotnet.Models.Dealer>
    <h2> Detail </h2>
    @using(Html.BeginForm()){

    <table class="table">
        <tr>
            <th>@Html.DisplayNameFor(m=>m.ID)</th>
            <th>@Html.DisplayNameFor(m=>m.Name)</th>
            <th>@Html.DisplayNameFor(m=>m.RegistrationDate)</th>
            <th>@Html.DisplayNameFor(m=>m.AutoPartsName)</th>
            <th>@Html.DisplayNameFor(m=>m.Manufacturer)</th>
            <th>@Html.DisplayNameFor(m=>m.MobileNumber)</th>
            <th>@Html.DisplayNameFor(m=>m.Email)</th>
            <th>@Html.DisplayNameFor(m=>m.Description)</th>
        </tr>
        <tbody>
            @foreach (var item in Model)
            {
                
            <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.RegistrationDate</td>
                <td>@item.AutoPartsName</td>
                <td>@item.Manufacturer</td>
                <td>@item.MobileNumber</td>
                <td>@item.Email</td>
                <td>@item.Description</td>
                <td><a asp-controller="Dealer" asp-action="Edit" asp-route-id="@item.ID"/>Edit</td>
                <td><a asp-controller="Dealer" asp-action="Delete" asp-route-id="@item.ID"/>Delete</td>
            </tr>
            }
        </tbody>
    </table>
    @Html.ActionLink("Add Dealer","Create","Dealer")
    }